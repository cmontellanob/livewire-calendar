{
    "name": "cmontellanob/livewire-calendar",
    "description": "Laravel Livewire calendar component fork",
    "keywords": [
        "cmontellanob",
        "livewire-calendar"
    ],
    "homepage": "https://github.com/asantibanez/livewire-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Carlos Montellano",
            "email": "carlosmontellano@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^2.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
        
    },
    "autoload": {
        "psr-4": {
            "Cmontellanob\\LivewireCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cmontellanob\\LivewireCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cmontellanob\\LivewireCalendar\\LivewireCalendarServiceProvider"
            ],
            "aliases": {
                "LivewireCalendar": "Cmontellanob\\LivewireCalendar\\LivewireCalendarFacade"
            }
        }
    }
}
